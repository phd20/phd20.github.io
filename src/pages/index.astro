---
import PageMeta from "../components/PageMeta.astro";
import PageLayout from "../layouts/PageLayout.astro";
import Intro from "../components/Intro.astro";
import Subscribe from "../components/Subscribe.astro"
import { SITE_TITLE } from "../config";
import Obsidian from "src/components/Obsidian.astro";
import Worldbuilding from "src/components/Worldbuilding.astro";
import Other from "src/components/Other.astro";
---

<PageLayout>
  <PageMeta title={`${SITE_TITLE} | TTRPG Creator`} slot="meta" />
  <Fragment slot="main">
    <Intro />
    <hr />
    <br />
    <Subscribe />
    <Obsidian />
    <Worldbuilding />
    <Other />
  </Fragment>
</PageLayout>
