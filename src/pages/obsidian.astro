---
import PageLayout from '../layouts/PageLayout.astro';
import PageMeta from '../components/PageMeta.astro';
import { SITE_TITLE } from '../config';
import TaggedBlogPosts from 'src/components/TaggedBlogPosts.astro';
import Subscribe from 'src/components/Subscribe.astro';

---

<PageLayout>
  <PageMeta title={`Obsidian | ${SITE_TITLE}`} slot="meta" />
  <section slot="main">
    <Subscribe />
    <TaggedBlogPosts title="ObsidianMD for Dungeons & Dragons" tag="obsidian" />
  </section>
</PageLayout>
